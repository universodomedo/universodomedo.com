{
  "compilerOptions": {
    "baseUrl": "frontend/src",
    "paths": { // para adicionar novos paths, tem que adicionar no tsconfig.json e no vite.config.ts
      "ApiConsumer/*": ["ApiConsumer/*"],
      "Assets/*": ["Assets/*"],
      "Classes/*": ["Classes/*"],
      "Componentes/*": ["Componentes/*"],
      "Contextos/*": ["Contextos/*"],
      "Layouts/*": ["Layouts/*"],
      "Paginas/*": ["Paginas/*"],
      "Providers/*": ["Providers/*"],
      "Recursos/*": ["Recursos/*"],
      "Redux/*": ["Redux/*"],
      "Servicos/*": ["Servicos/*"],
      "Uteis/*": ["Uteis/*"],
    },
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "target": "ES2020",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    // "module": "commonjs",
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "traceResolution": true,
    "types": ["node"],
    "skipLibCheck": true,
    "typeRoots": ["./node_modules/@types", "./frontend/src"],
    "forceConsistentCasingInFileNames": true
  },
  "include": ["frontend/src/**/*", "websocket/**/*.ts", "frontend/src/Types/classes/Item"],
}